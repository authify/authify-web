          <% @apikeys.each do |key| %>
          <tr>
            <td><%= key.attributes[:access_key] %></td>
            <td>
              <%= link_to remove_api_key_path(key.id), method: :post, data: { no_turbolink: true, turbolinks: false } do %>
                <%= icon('trash') %>
              <% end %>
            </td>
          </tr>
          <% end %>
